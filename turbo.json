{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "@ai16z/client-mastodon#build": {
            "outputs": ["dist/**"],
            "dependsOn": ["@ai16z/eliza#build"]
        },
        "@ai16z/client-misskey#build": {
            "outputs": ["dist/**"],
            "dependsOn": ["@ai16z/eliza#build"]
        },
        "test": {
            "dependsOn": ["build"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
